spring:
  datasource:
    username: ${CONTAINER.USERNAME}
    password: ${CONTAINER.PASSWORD}
    url: ${CONTAINER.URL}
  liquibase:
    enabled: false
  jpa:
    show-sql: true
  rabbitmq:
    host: ${CONTAINER.HOST}
    port: ${CONTAINER.PORT}
    username: ${CONTAINER.USERNAME}
    password: ${CONTAINER.PASSWORD}
  mail:
    host: smtp.gmail.com
    port: 587
    username: mail
    password: password
    properties:
      mail:
        smtp:
          auth: true
          starttls:
            enable: true
